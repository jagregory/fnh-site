---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  
  <title>Fluent NHibernate Blog</title>
  <link href="http://blog.fluentnhibernate.org/atom.xml" rel="self"/>
  <link href="http://blog.fluentnhibernate.org.org/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://blog.fluentnhibernate.org/</id>
  <author>
    <name>James Gregory</name>
    <email>james@jagregory.com</email>
  </author>

{% for post in site.posts %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://blog.fluentnhibernate.org{{ post.url }}"/>
    <updated>{{ post.date | date_to_xmlschema }}</updated>
    <id>http://blog.fluentnhibernate.org{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
{% endfor %}

</feed>
